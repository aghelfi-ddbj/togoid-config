# Endpoint: https://integbio.jp/rdf/mirror/ebi/sparql
PREFIX cco: <http://rdf.ebi.ac.uk/terms/chembl#>

SELECT DISTINCT ?chembl_id ?pubchem_id
FROM <http://rdf.ebi.ac.uk/dataset/chembl>
WHERE {
  ?molecule
    a cco:SmallMolecule ;
    cco:chemblId ?chembl ;
    cco:moleculeXref ?pubchem .
  FILTER(CONTAINS(STR(?pubchem), "pubchem.ncbi.nlm.nih.gov/substance/"))
  BIND (replace(?chembl, "CHEMBL", "") AS ?chembl_id)
  BIND (replace(str(?pubchem), "http://pubchem.ncbi.nlm.nih.gov/substance/", "") AS ?pubchem_id) }
