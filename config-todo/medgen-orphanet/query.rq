##endpoint https://togovar-dev.biosciencedbc.jp/sparql
PREFIX mo: <http://med2rdf/ontology/medgen#>
PREFIX dct: <http://purl.org/dc/terms/>
SELECT DISTINCT ?medgen_id ?orphanet_id
WHERE {
  ?medgen_uri dct:identifier ?medgen_id ;
              mo:mgconso ?mgconso .
  ?mgconso dct:source mo:ORDO ;
           rdfs:seeAlso ?orphanet .
    BIND (replace(str(?orphanet), 'http://www.orpha.net/ORDO/Orphanet_', '') AS ?orphanet_id)
}