# endpoint: https://togovar-dev.biosciencedbc.jp/sparql
PREFIX mo: <http://med2rdf/ontology/medgen#>
PREFIX dct: <http://purl.org/dc/terms/>
SELECT DISTINCT ?medgen_id ?mesh_id
WHERE {
  ?medgen_uri dct:identifier ?medgen_id ;
              mo:mgconso ?mgconso .
  ?mgconso dct:source mo:MSH ;
           rdfs:seeAlso ?mesh .
  BIND (replace(str(?mesh), 'http://id.nlm.nih.gov/mesh/', '') AS ?mesh_id)
}
